package MergeSort;

public class aulaMargeSortFuncao {
//	 int v [] = {5,6,2,7,9,1};
	 
	 public static void teste(int v []) {
		 int aux;
		 for(int i = 0; i < v.length-1; i++) {
			 if(v[i] > v[i+ 1]) {
				aux = v[i];
				v[i] = v[i+1];
				v[i+1] = aux;
				
			 }
			 while(v[0] < v[i+1]) {
				 aux = v[i+1];
					v[i+2] = v[i+1];
					v[i+1] = aux;
			 }
		 }
		 
	 }
	 
	 public static void imprimir (int[] v){
		 for(int i = 0; i < v.length; i++) {
			 System.out.print(v[i]+" ");
		 }
	 }
	 
	
	 
	 public static void main(String[] args) {
		 int v[] = {5,6,2,7,9,1};
		 teste(v);
		 imprimir(v);
		}
	

}
