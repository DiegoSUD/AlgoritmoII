package MergeSort;

public class aulaMargeSortFuncao {
//	 int v [] = {5,6,2,7,9,1};
	 
	 public static void teste(int v []) {
		 int pos;
		 for(int i = 0; i < v.length; i++) {
			 if(v[i] > v[i+ 1]) {
				pos = v[i];
				v[i + 1] = pos;
				v[i] = v[i + 1];
				
			 }
		 }
		 
	 }
	 
	 public static void imprimir (int[] v){
		 for(int i = 0; i < v.length; i++) {
			 System.out.print(v[i]+" ");
		 }
	 }
	 
	
	 
	 public static void main(String[] args) {
		 int v[] = {5,6,2,7,9,1};
//		 teste(v);
		 imprimir(v);
		}
	

}
